    "tailwindcss": "^3.4.4",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/react": "^3.6.0",
    "@astrojs/sitemap": "^3.1.6",
